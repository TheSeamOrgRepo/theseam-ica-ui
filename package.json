{
  "name": "theseam-ica-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -s clean -p ica-ui:build:watch ica-ui:copy:assets:watch ica-ui:copy:styles:watch serve",
    "serve": "npm-run-all -s wait-for-ica-ui-styles serve2",
    "serve2": "ng serve --no-live-reload",
    "build": "ng build",
    "ica-ui:build": "ng build ica-ui && npm run ica-ui:copy:assets && npm run ica-ui:copy:styles",
    "ica-ui:build:watch": "ng build ica-ui --watch",
    "wait-for-ica-ui": "just-wait -p \"dist/ica-ui/**\" -t 120",
    "wait-for-ica-ui-assets": "just-wait -p \"dist/ica-ui/assets\" -t 120",
    "wait-for-ica-ui-styles": "just-wait -p \"dist/ica-ui/styles.scss\" -t 120",
    "ica-ui:copy:assets": "cpx -p projects/ica-ui/src/assets/**/* dist/ica-ui/assets",
    "ica-ui:copy:assets:watch": "npm-run-all -s wait-for-ica-ui ica-ui:copy:assets:watch2",
    "ica-ui:copy:assets:watch2": "echo \"Starting Assets Watcher\" & cpx -p projects/ica-ui/src/assets/**/* dist/ica-ui/assets --watch",
    "ica-ui:copy:styles": "cpx -p projects/ica-ui/src/styles/**/* dist/ica-ui/styles",
    "ica-ui:copy:styles2": "cpx -p projects/ica-ui/src/*.scss dist/ica-ui",
    "ica-ui:copy:styles:watch": "npm-run-all -s wait-for-ica-ui-assets ica-ui:copy:styles:watch2",
    "ica-ui:copy:styles:watch2": "echo \"Starting Styles Watchers\" & npm-run-all -p ica-ui:copy:styles:watch3 ica-ui:copy:styles:watch4",
    "ica-ui:copy:styles:watch3": "echo \"Starting Styles Watcher 1\" & cpx -p projects/ica-ui/src/styles/**/* dist/ica-ui/styles --watch",
    "ica-ui:copy:styles:watch4": "echo \"Starting Styles Watcher 2\" & cpx -p projects/ica-ui/src/styles.scss dist/ica-ui --watch",
    "clean": "rimraf dist/*",
    "clean:lib": "rimraf dist/ica-ui/*",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gulp": "gulp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/router": "^7.0.4",
    "@ng-select/ng-select": "^2.12.0",
    "angular2-signaturepad": "^2.8.0",
    "angular6-json-schema-form": "^1.0.4",
    "core-js": "^2.5.7",
    "css-element-queries": "^1.1.1",
    "del": "^3.0.0",
    "json-ptr": "^1.1.1",
    "just-wait": "^1.0.11",
    "ng-animate": "^0.3.4",
    "ngx-json-viewer": "^2.4.0",
    "ngx-take-until-destroy": "^5.2.3",
    "npm-run-all": "^4.1.3",
    "pdfjs-dist": "^2.0.943",
    "pdfmake": "^0.1.39",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "strip-json-comments": "^2.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular-devkit/build-ng-packagr": "^0.10.6",
    "@angular/cli": "^7.0.6",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/language-service": "^7.0.4",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.9",
    "codelyzer": "^4.5.0",
    "cpx": "^1.5.0",
    "fancy-log": "^1.3.2",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.0",
    "outdent": "^0.7.0",
    "protractor": "^5.4.1",
    "shelljs": "^0.8.3",
    "ts-node": "^7.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "3.1.6"
  }
}
